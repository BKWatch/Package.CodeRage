  xsi:schemaLocation="http://www.coderage.com/2008/project
  Build/Resource/project.xsd">
  <info>
    <label>CodeRage Tools</label>
    <description>Common tools shared by all CodeRage projects</description>
    <copyright>2008 CodeRage</copyright>
    <version>2.0</version>
